@model mvc22myown.Models.UserDetail
@{
    ViewBag.Title = "RegisterView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RegisterView</h2>

<html>
<head>
    <title>Register</title>
</head>
<body>
    <div>
        @using (Html.BeginForm("SaveRegistrationDetails", "Registration", FormMethod.Post))
        {
            @Html.LabelFor(c => c.UserName)
            @Html.TextBoxFor(x => x.UserName)
            <br /> @*Here UserName ,Password is taken from UserDetail Class *@
            @Html.LabelFor(c => c.Password)
            @Html.PasswordFor(x => x.Password)
            <br />
            <input type="submit" name="BtnSubmit" value="Register" />
        }
        @*This returns UserDetail obbject*@
    </div>
</body>
</html>

